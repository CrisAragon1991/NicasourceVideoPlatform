{
   "name": "typeorm-sample",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "type": "commonjs",
   "devDependencies": {
      "@types/express": "^4.17.17",
      "@types/jest": "^29.5.3",
      "@types/node": "^16.18.40",
      "@types/supertest": "^2.0.12",
      "@typescript-eslint/eslint-plugin": "^6.3.0",
      "@typescript-eslint/parser": "^6.3.0",
      "eslint": "^8.47.0",
      "jest": "^29.6.2",
      "nodemon": "^3.0.1",
      "supertest": "^6.3.3",
      "ts-jest": "^29.1.1",
      "ts-node": "^10.7.0",
      "typescript": "^4.9.5"
   },
   "dependencies": {
      "@types/bcrypt": "^5.0.0",
      "@types/jsonwebtoken": "^9.0.2",
      "bcrypt": "^5.1.0",
      "body-parser": "^1.19.1",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.0",
      "cors": "^2.8.5",
      "dotenv": "^16.3.1",
      "express": "^4.17.2",
      "express-fileupload": "^1.4.0",
      "jsonwebtoken": "^9.0.1",
      "morgan": "^1.10.0",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.17"
   },
   "scripts": {
      "start": "ts-node src/index.ts",
      "dev": "npm run m:run && nodemon -w *.ts -w .env src/index.ts",
      "build": "tsc",
      "typeorm": "typeorm-ts-node-commonjs -d ./src/data-source.ts",
      "m:gen": "npm run typeorm migration:generate",
      "m:cre": "typeorm-ts-node-commonjs migration:create",
      "m:run": "npm run typeorm migration:run",
      "m:rev": "npm run typeorm migration:revert",
      "test": "jest --watchAll --collectCoverage",
      "lint": "eslint . --ext .ts",
      "lint.fix": "eslint . --ext .ts --fix"
   }
}
